{
  "ci": {
    "collect": {
      "url": ["http://localhost:3000/"],
      "startServerCommand": "npm run start"
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 1.0}],
        "categories:seo": ["error", {"minScore": 0.92}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
